   "source": [
    "# Get MAX stops using Fourquare API"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "client_id = [\"client_id\"]\n",
    "client_secret = [\"client_secret\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "client = foursquare.Foursquare(client_id = client_id,\n",
    "                               client_secret=client_secret,\n",
    "                               version='20191021')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "response = client.venues.search(params={'query': \"Trimet MAX Station\",\n",
